{
  "name": "js-starter",
  "version": "0.0.1",
  "description": "Just a starter project for JS apps.",
  "homepage": "https://github.com/ngobach/js-starter",
  "repository": {
    "type": "git",
    "url": "https://github.com/ngobach/js-starter"
  },
  "main": "dist/index.js",
  "devDependencies": {
    "ava": "^2.4.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-dynamic-import-node": "^2.1.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-ava": "^9.0.0",
    "eslint-plugin-import": "^2.14.0",
    "sinon": "^7.5.0"
  },
  "scripts": {
    "start": "node ./dist/index.js",
    "dev": "babel-node ./src/index.js",
    "build": "babel src --out-dir dist",
    "lint": "eslint ./src",
    "test": "ava"
  },
  "keywords": [
    "starter"
  ],
  "author": "BachNX <mail@ngobach.com>",
  "license": "ISC",
  "files": [
    "./dist"
  ],
  "ava": {
    "files": [
      "src/**/__test__/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
