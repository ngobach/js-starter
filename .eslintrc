{
  "env": {
    "es6": true,
    "commonjs": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "parser": "babel-eslint",
  "extends": "airbnb-base",
  "plugins": [
    "dynamic-import-node",
    "ava"
  ],
  "rules": {
    "no-console": "off"
  }
}
